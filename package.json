  },
  "devDependencies": {
    "@types/jest": "^21.1.10",
    "@types/ramda": "github:types/npm-ramda",
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-filehash": "^6.9.4",
